
<script type="text/javascript" src="@Url.Content("~/Content/themes/dashboard/dashboard.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Content/themes/dashboard/maps.js")"></script>

<h3>Dashboard</h3>

@Html.Partial("_DashboardNavigation")

<div id="dashboard-index">
    @Html.Action("_SongsPlayed")
</div>

<script type="text/javascript">
    $(document).ready(function () {
        loadScript();

        $("ul#dashboard-nav > li > a").bind("click", function () {
            $("ul#dashboard-nav > li > a").removeClass('selected');
            $(this).addClass('selected');
        });
    });
</script>

