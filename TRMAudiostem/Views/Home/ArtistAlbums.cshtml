@model DomainModel.Entities.Artist

@{
    var TRMWebService = new TRMWebService.TRMWCFWebServiceJson();
    var albums = TRMWebService.GetArtistAlbums((DomainModel.Entities.Artist)Model);
    foreach(var album in albums)
    {
        <li class="dot">
            <a href="#">@album.AlbumTitle</a>
            <div class="mp-level">
                <h2>@album.AlbumTitle</h2>
                <a class="mp-back" href="#"><i class="fa fa-arrow-left"></i>Back</a>
                <ul>
                    @Html.Partial("ArtistSongs", album)
                </ul>
            </div>
        </li>
    }
}

